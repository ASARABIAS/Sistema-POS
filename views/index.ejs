   <main>
    <section class="sold">
        <div class="table">
            <div class="table_header">
                <p>Product Details</p>
                <div> <input placeholder="product" /> <button class="add_new">+ Add New</button> </div>
            </div>
            <div class="table_section">
                <table>
                    <thead>
                        <tr>
                            <th>Codigo</th>
                            <th>Producto</th>
                            <th>Precio unitario</th>
                            <th>Cantidad</th>
                            <th>Precio total</th>
